generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  name      String
  password  String
}

model Feed {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  url   String  @unique
  items Json
}